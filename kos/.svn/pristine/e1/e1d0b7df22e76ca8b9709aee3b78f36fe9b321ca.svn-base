.kiosk-1-3,
.kiosk-1-3 * {
  box-sizing: border-box;
}
.kiosk-1-3 {
  background: #ffffff;
  height: 1696.91px;
  position: relative;
  overflow: hidden;
}
.kiosk-1-3__div {
  text-align: center; /* 텍스트 중앙 정렬 */
  font-family: "Pretendard-Bold", sans-serif; /* 폰트 패밀리 설정 */
  font-size: 88px; /* 글자 크기 설정 */
  font-weight: 700; /* 글자 두께 설정 (Bold) */
  position: absolute; /* 절대 위치 지정 */
  right: 14%; /* 오른쪽에서 14% 위치 설정 */
  left: 14%; /* 왼쪽에서 14% 위치 설정 */
  width: 72%; /* 너비를 72%로 설정 */
  bottom: 88%; /* 아래에서 88% 위치 설정 */
  top: 5%; /* 위쪽에서 5% 위치 설정 */
  height: 6%; /* 높이를 6%로 설정 */
}

.div-span,
.div-span2 {
  display: inline; /* 요소를 인라인으로 설정하여 한 줄로 나열 */
  color: #000000; /* 기본 글자 색상 */
  font-family: "Pretendard-Bold", sans-serif; /* 폰트 패밀리 설정 */
  font-size: 82px; /* 글자 크기 설정 (변경하지 않음) */
  font-weight: 700; /* 글자 두께 설정 (Bold) */
  margin: 0; /* 기본 마진 제거 */
}

.div-span {
  letter-spacing: -8px; /* 자간 줄이기 (값을 조정하여 간격 조절) */
}

.div-span2 {
  color: #ae202a; /* 두 번째 글자 색상 설정 */
  letter-spacing: -8px; /* 자간 줄이기 (값을 조정하여 간격 조절) */
  margin-left: -18px; /* 왼쪽 여백을 음수로 설정하여 간격 줄이기 */
}



